{
    "env": {
        "browser": true,
        "es2020": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "import/no-extraneous-dependencies": 0,
        "no-console": 0,
        "no-underscore-dangle": 0,
        "no-restricted-globals": 0,
        "linebreak-style": 0,
        "no-unused-vars":"off",
        "no-empty-function":"off",
        "class-methods-use-this": 0,
        "no-new":"off",
        "no-undef":"off",
        "no-plusplus": 0,
        "no-await-in-loop": 0,
        "consistent-return": "off",
        "no-prototype-builtins":"off",
        "import/prefer-default-export": 0,
        "no-return-assign": 0
    }
}
